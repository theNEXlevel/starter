{
  "name": "starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint:all": "nx run-many --target=lint --project=all",
    "test:all": "nx run-many --target=test --project=all",
    "build:all": "nx run-many --target=build --projects=ui,api --configuration=production",
    "e2e": "pnpm db:dev && nx run e2e:e2e",
    "prod": "pnpm run lint:all && pnpm run build:all && pnpm run test:all && pnpm db:dev && nx run e2e:e2e",
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:rm": "docker compose -f tools/scripts/docker-compose.yml rm dev-db -sfv",
    "db:dev:up": "docker compose -f tools/scripts/docker-compose.yml up dev-db -d",
    "db:dev": "pnpm run db:dev:rm && pnpm run db:dev:up && sleep 1 && npx prisma generate && pnpm run prisma:dev:deploy",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.0.3",
    "@angular/cdk": "15.0.2",
    "@angular/common": "15.0.3",
    "@angular/compiler": "15.0.3",
    "@angular/core": "15.0.3",
    "@angular/forms": "15.0.3",
    "@angular/material": "15.0.2",
    "@angular/platform-browser": "15.0.3",
    "@angular/platform-browser-dynamic": "15.0.3",
    "@angular/router": "15.0.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.0.5",
    "@ngrx/effects": "^14.3.0",
    "@ngrx/store": "^14.3.0",
    "@ngrx/store-devtools": "^14.3.0",
    "@nrwl/angular": "15.3.0",
    "argon2": "^0.28.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "husky": "^8.0.1",
    "ngrx-store-localstorage": "^14.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pretty-quick": "^3.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.3",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "15.0.3",
    "@angular/language-service": "15.0.3",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "15.3.0",
    "@nrwl/cypress": "15.3.0",
    "@nrwl/eslint-plugin-nx": "15.3.0",
    "@nrwl/jest": "15.3.0",
    "@nrwl/linter": "15.3.0",
    "@nrwl/nest": "15.3.0",
    "@nrwl/node": "15.3.0",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/workspace": "15.3.0",
    "@prisma/client": "^4.1.1",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.1",
    "@types/passport-jwt": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "5.36.1",
