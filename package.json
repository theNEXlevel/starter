    "cypress": "^11.0.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "^8.0.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "12.2.3",
    "ng-packagr": "15.0.3",
    "nx": "15.3.0",
    "postcss": "^8.4.31",
    "postcss-import": "~14.1.0",
